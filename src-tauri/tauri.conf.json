{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "JOPLdoro",
  "version": "1.1.0",
  "identifier": "nl.triffit.JOPLdoro.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "JOPLdoro",
        "width": 180,
        "height": 40,
        "resizable": false,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/jopl.ico"
    ]
  }
}
